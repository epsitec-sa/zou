<?xml version="1.0" encoding="utf-8"?>
<!-- This property imports zou standard property sheets. -->
<Project ToolsVersion="4.0" InitialTargets="CppOutDirTrace" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Condition="'$(Zou)' == ''" Project="zou.props" />

  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] Standard properties</_PropertySheetDisplayName>
  </PropertyGroup>

  <ImportGroup Label="PropertySheets">
    <Import Project="$(ZouDir)Cpp.NTVersion.props" />
    <Import Project="$(ZouDir)Cpp.IntDir.props" />
    <Import Project="$(ZouDir)Cpp.OutDir.props" />
  </ImportGroup>
</Project>