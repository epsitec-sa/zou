<?xml version="1.0" encoding="utf-8"?>
<!-- This property imports zou standard property sheets. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Condition="'$(Zou)' == ''" Project="zou.props" />

  <PropertyGroup>
    <_PropertySheetDisplayName Condition="'$(ZouPack)' != 'true' And '$(ZouImport)' != 'true'">[zou] Standard settings</_PropertySheetDisplayName>
    <_PropertySheetDisplayName Condition="'$(ZouPack)'   == 'true'">[zou] Pack settings</_PropertySheetDisplayName>
    <_PropertySheetDisplayName Condition="'$(ZouImport)' == 'true'">[zou] Import settings</_PropertySheetDisplayName>
  </PropertyGroup>

  <!-- WARNING: do not change import order -->
  <ImportGroup Label="PropertySheets">
    <Import Project="$(ZouDir)Cpp.NTVersion.props" />
    <Import Project="$(ZouDir)Com.WorkDir.props" />
    <Import Project="$(ZouDir)Cpp.IntDir.props" />
    <Import Project="$(ZouDir)Cpp.IncludeDirs.props" Condition="'$(ZouPack)' != 'true' And '$(ZouImport)' != 'true'" />
    <Import Project="$(ZouDir)Cpp.DbgSettings.props" Condition="'$(ZouPack)' != 'true' And '$(ZouImport)' != 'true'" />
    <Import Project="$(ZouDir)Cpp.DbgDir.props"      Condition="'$(ZouImport)' != 'true'" />
    <Import Project="$(ZouDir)Cpp.PkgDir.props"      Condition="'$(ZouPack)' == 'true'" />
    <Import Project="$(ZouDir)Cpp.OutDir.props" />
  </ImportGroup>
</Project>