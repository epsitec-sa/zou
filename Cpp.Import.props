<?xml version="1.0" encoding="utf-8"?>
<!-- Import this script at the beginning of a C++ .import agent -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ZouAgentFunc>Import</ZouAgentFunc>
    <_PropertySheetDisplayName>[zou] C++ Import Agent</_PropertySheetDisplayName>
  </PropertyGroup>

  <ImportGroup Label="ZouExtensions">
    <Import Project="Cpp.Utility.props" />
  </ImportGroup>

  <ImportGroup Label="PropertySheets">
    <Import Project="Cpp.NTVersion.props" />
    <Import Project="Com.WorkDir.props"/>
    <Import Project="Cpp.IntDir.props" />
    <Import Project="Cpp.OutDir.props"/>
  </ImportGroup>
  
  <Import Project="Cpp.Utility.targets" />
</Project>