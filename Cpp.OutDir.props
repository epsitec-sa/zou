<?xml version="1.0" encoding="utf-8"?>
<!--
Import this MSBuild property sheet into your project to define the default 'OutDir' and 'IntDir' properties.
For example:

  <ImportGroup Label="PropertySheets">
    <Import Project="MsBuild\Cpp.OutDir.props" />
  </ImportGroup>

-->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Output Directories</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(SolutionDir)$(Platform)\$(PlatformToolset)\$(Configuration)\</OutDir>
    <IntDir>$(Platform)\$(PlatformToolset)\$(Configuration)Tmp\</IntDir>
  </PropertyGroup>
</Project>