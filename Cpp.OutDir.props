<?xml version="1.0" encoding="utf-8"?>
<!--
Import this MSBuild property sheet into your project to define the default 'OutDir' and 'IntDir' properties.
For example:

  <ImportGroup Label="PropertySheets">
    <Import Project="zou\Cpp.OutDir.props" />
  </ImportGroup>

-->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>zou - Output Directories</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(SolutionDir)$(Platform)\$(PlatformToolset)\$(Configuration)\</OutDir>
    <!--
    The IntDir property ends with the project name to avoid mixing intermediate files
    when projects are located in the same folder and to suppress some build warnings.
    -->
    <IntDir>$(Platform)\$(PlatformToolset)\$(Configuration)\$(ProjectName)\</IntDir>
  </PropertyGroup>
</Project>