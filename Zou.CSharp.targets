<?xml version="1.0" encoding="utf-8"?>
<!-- Import this script at the end of the C# interop project -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ZouImportProject>@(ImportProject)</ZouImportProject>
    <ZouPoResource>@(PoResource)</ZouPoResource>
  </PropertyGroup>
  <Import Condition="'$(ZouImportProject)' != ''" Project="Cs.Interop.targets" />
  <Import Condition="'$(ZouPoResource)'    != ''" Project="Cs.PoResource.targets" />
</Project>