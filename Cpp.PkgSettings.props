<?xml version="1.0" encoding="utf-8"?>
<!-- This property imports zou packaging property sheets for C++ projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Condition="'$(Zou)' == ''" Project="zou.props" />

  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] Pack settings</_PropertySheetDisplayName>
  </PropertyGroup>

  <!-- WARNING: do not change import order -->
  <ImportGroup Label="PropertySheets">
    <Import Project="$(ZouDir)Cpp.NTVersion.props" />
    <Import Project="$(ZouDir)WorkDir.props" />
    <Import Project="$(ZouDir)Cpp.IntDir.props" />
    <Import Project="$(ZouDir)Cpp.DbgDir.props" />
    <Import Project="$(ZouDir)Cpp.OutDir.props" />
    <Import Project="$(ZouDir)Cpp.PkgDir.props" />
  </ImportGroup>
</Project>