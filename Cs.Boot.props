<?xml version="1.0" encoding="utf-8"?>
<!-- The Cs.Boot.targets is used to boot zou in a C++ utility project for a C# project -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Condition="'$(Zou)' == ''" Project="zou.props" />
  <!-- Force Visual Studio to hide the default platform toolset '(Visual Studio 2010)' in the title of the parent project. -->
  <PropertyGroup>
    <PlatformToolset>v140</PlatformToolset>
  </PropertyGroup>
</Project>