<?xml version="1.0" encoding="utf-8"?>
<!-- C# pack agent : builds and packages main application into the C# container (Payment) -->
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\Cs.Boot.props" />
  
  <PropertyGroup Label="Globals">
    <ProjectGuid>{CE838607-7D36-4593-93FA-EAAEF0B74148}</ProjectGuid>
  </PropertyGroup>

  <ImportGroup Label="PropertySheets">
    <Import Project="$(ZouDir)Cs.Pack.props" />
  </ImportGroup>

  <!-- build package -->
  <PropertyGroup>
    <!-- Redirect product binaries to its installation folder -->
    <PkgProdBinDir>$(ZouPrivateDir)</PkgProdBinDir>
  </PropertyGroup>
  <ItemGroup>
    <ProdBinExclude Include="$(OutDir)**\*.xml" />
  </ItemGroup>
  <Import Project="$(ZouDir)Cs.Pack.targets" />
</Project>