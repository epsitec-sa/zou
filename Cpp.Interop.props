<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] C++ Interop Agent</_PropertySheetDisplayName>
  </PropertyGroup>
  
  <ImportGroup Label="ZouExtensions">
    <Import Project="Cpp.Utility.props" />
  </ImportGroup>

  <PropertyGroup>
    <InteropProxy Condition="'$(InteropProxy)' == ''">$(ProjectName)</InteropProxy>
  </PropertyGroup>
  
  <ImportGroup Label="PropertySheets">
    <Import Project="Cpp.NTVersion.props" />
    <Import Project="Com.WorkDir.props" />
    <Import Project="Tmp.IntDir.props" />
    <Import Project="Com.InteropPlatform.props" />
    <Import Project="Com.InteropDir.props" />
  </ImportGroup>

  <PropertyGroup>
    <OutDir>$(InteropDir)</OutDir>
    <ForwardOutDir Condition="'$(ForwardOutDir)' == ''">$(OutDir)</ForwardOutDir>
  </PropertyGroup>

  <Import Project="Cpp.Utility.targets" />
</Project>