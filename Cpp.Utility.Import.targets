<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- Create a minimal C++ utility project -->
  <Import Project="$(MSBuildThisFileDirectory)Cpp.Utility.Base.targets" />

  <!-- Redirect standard targets -->
  <Target Name="Clean"   DependsOnTargets="ExportClean" />
  <Target Name="Build"   DependsOnTargets="ExportBuild" />
  <Target Name="Rebuild" DependsOnTargets="ExportRebuild" />
</Project>