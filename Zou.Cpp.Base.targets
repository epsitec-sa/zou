<!--
Import this script at the end of a C++ project instead of Zou.Cpp.targets
when the project does not need nor nuget, nor boost, nor VLD (ex: help project)
 
  [Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets"/]
  
  [ImportGroup Label = "ExtensionTargets"]
    [Import Project = "$(ZouDir)Zou.Cpp.Base.targets"/]
  [/ ImportGroup]
 
-->
<Project>
  <Import Project="$(MSBuildThisFileDirectory)Cpp.Ducible.targets"/>
  <Import Project="$(MSBuildThisFileDirectory)ImportFile.targets"/>
</Project>
