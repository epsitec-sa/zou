<?xml version="1.0" encoding="utf-8"?>
<!--
This property sheet define the default 'OutDir' and 'IntDir' properties for C++ projects.
-->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>zou - Output Directories (default)</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(SolutionDir)$(Platform)\$(PlatformToolset)\$(Configuration)\</OutDir>
    <!-- Avoid to share the intermediate folder between projects located int he same folder -->
    <IntDir>$(Platform)\$(PlatformToolset)\obj\$(Configuration)\$(ProjectName)\</IntDir>
  </PropertyGroup>
</Project>