<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default configuration settings for C++ projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <_PropertySheetDisplayName>[zou] Debug Libraries, Incremental Linking</_PropertySheetDisplayName>
    <_Configuration>true</_Configuration>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <_PropertySheetDisplayName>[zou] Release Libraries</_PropertySheetDisplayName>
    <_Configuration>true</_Configuration>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Debug'" Label="Configuration">
    <UseDebugLibraries>true</UseDebugLibraries>
    <LinkIncremental>true</LinkIncremental>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'" Label="Configuration">
    <UseDebugLibraries>false</UseDebugLibraries>
    <LinkIncremental>false</LinkIncremental>
  </PropertyGroup>
</Project>