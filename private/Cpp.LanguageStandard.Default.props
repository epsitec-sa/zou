<?xml version="1.0" encoding="utf-8"?>
<!--
This property sheet defines the default language standard for C++ projects.
stdcpp14
stdcpp17
stdcpp20
stdcpplatest
-->
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] ISO C++ 20 Standard</_PropertySheetDisplayName>
    <_LanguageStandard>true</_LanguageStandard>
  </PropertyGroup>

  <ItemDefinitionGroup>
    <ClCompile>
      <LanguageStandard>stdcpp20</LanguageStandard>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>
