<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default 'WorkDir' property for C++ and C# projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] WorkDir = $(SolutionDir)</_PropertySheetDisplayName>
    <WorkDir Condition="'$(WorkDir)' == ''">$(SolutionDir)</WorkDir>
  </PropertyGroup>
</Project>