<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default debug directory for C++ projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Choose>
    <When Condition="$(SolutionDir) == '*Undefined*'">
      <PropertyGroup>
        <_PropertySheetDisplayName>[zou] DbgDir = $(Platform)\$(PlatformToolset)\obj\$(Configuration)\</_PropertySheetDisplayName>
        <DbgDir>$(Platform)\$(PlatformToolset)\obj\$(Configuration)\</DbgDir>
      </PropertyGroup>
    </When>
    <Otherwise>
      <PropertyGroup>
        <_PropertySheetDisplayName>[zou] DbgDir = $(SolutionDir)$(Platform)\$(PlatformToolset)\obj\$(Configuration)\</_PropertySheetDisplayName>
        <DbgDir>$(SolutionDir)$(Platform)\$(PlatformToolset)\obj\$(Configuration)\</DbgDir>
      </PropertyGroup>
    </Otherwise>
  </Choose>
</Project>