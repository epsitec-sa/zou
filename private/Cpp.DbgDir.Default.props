<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default debug directory for C++ projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] DbgDir = $(WorkDir)$(Platform)\$(PlatformToolset)\dbg\$(Configuration)\</_PropertySheetDisplayName>
    <DbgDir Condition="'$(DbgDir)' == ''">$(WorkDir)$(Platform)\$(PlatformToolset)\dbg\$(Configuration)\</DbgDir>
  </PropertyGroup>
</Project>