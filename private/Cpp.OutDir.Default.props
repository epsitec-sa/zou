<!-- This property sheet defines the default 'OutDir' property for C++ projects. -->
<Project>
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] OutDir = $(WorkDir)$(Platform)\$(PlatformToolset)\bin\$(Configuration)\</_PropertySheetDisplayName>
    <OutDir Condition="'$(OutDir)' == ''">$(WorkDir)$(Platform)\$(PlatformToolset)\bin\$(Configuration)\</OutDir>
  </PropertyGroup>
</Project>
