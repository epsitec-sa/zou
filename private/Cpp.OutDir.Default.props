<!-- This property sheet defines the default 'OutDir' property for C++ projects. -->
<Project>
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] OutDir = $(WorkDir)$(BinDir)</_PropertySheetDisplayName>
    <OutDir Condition="'$(OutDir)' == ''">$(WorkDir)$(BinDir)</OutDir>
  </PropertyGroup>
</Project>
