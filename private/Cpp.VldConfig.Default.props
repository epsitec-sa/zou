<?xml version="1.0" encoding="utf-8"?>
<!-- Visual Leak Detector Configuration -->
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)' == 'Debug'">
    <_PropertySheetDisplayName>[zou] EnableVLD = true</_PropertySheetDisplayName>

    <EnableVLD  Condition="'$(EnableVLD)'  == ''">true</EnableVLD>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)' == 'Release'">
    <_PropertySheetDisplayName>[zou] EnableVLD = false</_PropertySheetDisplayName>

    <EnableVLD  Condition="'$(EnableVLD)'  == ''">false</EnableVLD>
  </PropertyGroup>
</Project>