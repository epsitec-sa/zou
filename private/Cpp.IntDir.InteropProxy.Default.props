<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default 'IntDir' property for C++ interop proxy projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] IntDir = $(TMP)\_zou\$(BundleName)\$(BundleRelativeProjectPath)\</_PropertySheetDisplayName>

    <!-- Redirect IntDir to temp folder to avoid cluttering the solution directory. -->
    <IntDir>$(TMP)\_zou\$(BundleName)\$(BundleRelativeProjectPath)\</IntDir>
    <!-- Avoid MSB8029 warning -->
    <IgnoreWarnIntDirInTempDetected>true</IgnoreWarnIntDirInTempDetected>
  </PropertyGroup>
</Project>