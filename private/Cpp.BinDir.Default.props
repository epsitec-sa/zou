<!-- This property sheet defines the default 'BinDir' property for C++ projects. -->
<Project>
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] BinDir = bin\$(Configuration)\$(Platform)\</_PropertySheetDisplayName>
    <BinDir Condition="'$(BinDir)' == ''">bin\$(Configuration)\$(Platform)\</BinDir>
  </PropertyGroup>
</Project>
