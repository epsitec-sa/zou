<!-- This property sheet defines the default 'PkgDir' property for C++ projects. -->
<Project>
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] PkgDir = $(WorkDir)pkg\$(Configuration)\</_PropertySheetDisplayName>
    <PkgDir Condition="'$(PkgDir)' == ''">$(WorkDir)pkg\$(Configuration)\</PkgDir>
  </PropertyGroup>
</Project>
