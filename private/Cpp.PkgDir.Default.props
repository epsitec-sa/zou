<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default 'OutDir' property for C++ projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Choose>
    <When Condition="$(SolutionDir) == '*Undefined*'">
      <PropertyGroup>
        <_PropertySheetDisplayName>[zou] PkgDir = $(Platform)\$(PlatformToolset)\pkg\$(Configuration)\</_PropertySheetDisplayName>
        <PkgDir>$(Platform)\$(PlatformToolset)\pkg\$(Configuration)\</PkgDir>
      </PropertyGroup>
    </When>
    <Otherwise>
      <PropertyGroup>
        <_PropertySheetDisplayName>[zou] PkgDir = $(SolutionDir)$(Platform)\$(PlatformToolset)\pkg\$(Configuration)\</_PropertySheetDisplayName>
        <PkgDir>$(SolutionDir)$(Platform)\$(PlatformToolset)\pkg\$(Configuration)\</PkgDir>
      </PropertyGroup>
    </Otherwise>
  </Choose>
</Project>