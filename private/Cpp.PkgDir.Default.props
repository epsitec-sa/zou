<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default 'PkgDir' property for C++ projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] PkgDir = $(WorkDir)$(Platform)\$(PlatformToolSet)\pkg\$(Configuration)\</_PropertySheetDisplayName>
    <PkgDir>$(WorkDir)$(Platform)\$(PlatformToolSet)\pkg\$(Configuration)\</PkgDir>
  </PropertyGroup>
</Project>