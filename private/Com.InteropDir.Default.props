<!-- This property sheet defines the default 'InteropDir' property for C++ and C# projects. -->
<Project>
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] InteropDir = $(WorkDir)$(InteropPlatform)\$(InteropProxy)\$(Configuration)\</_PropertySheetDisplayName>
    <InteropDir Condition="'$(InteropDir)' == ''">$(WorkDir)$(InteropPlatform)\$(InteropProxy)\$(Configuration)\</InteropDir>
  </PropertyGroup>
</Project>
