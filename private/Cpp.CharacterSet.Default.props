<!-- This property sheet defines the default character set for C++ projects. -->
<Project>
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] Multi-Byte Character Set</_PropertySheetDisplayName>
    <_CharacterSet>true</_CharacterSet>
  </PropertyGroup>

  <PropertyGroup Label="Configuration">
    <CharacterSet>MultiByte</CharacterSet>
  </PropertyGroup>
</Project>
