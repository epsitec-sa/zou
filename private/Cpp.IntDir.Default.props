<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default 'IntDir' property for C++ projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] IntDir = $(Platform)\$(PlatformToolset)\obj\$(Configuration)\$(ProjectName)\</_PropertySheetDisplayName>
    
    <!-- $(ProjectName) is appended to $(IntDir) to avoid sharing the intermediate folder between projects located int he same folder -->
    <IntDir Condition="'$(IntDir)' == ''">$(Platform)\$(PlatformToolset)\obj\$(Configuration)\$(ProjectName)\</IntDir>
  </PropertyGroup>
</Project>