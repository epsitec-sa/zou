<!-- This property sheet defines the default 'PkgDir' property for go projects. -->
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] PkgDir = pkg\$(Platform)\$(Configuration)\</_PropertySheetDisplayName>
    <PkgDir Condition="'$(PkgDir)' == ''">pkg\$(Platform)\$(Configuration)\</PkgDir>
  </PropertyGroup>
</Project>