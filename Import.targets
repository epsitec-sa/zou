<!--
Import this script at the end of a C++ import agent.

This script imports the 'zou.build.targets' located in the invoking project directory.
There you can define the Itemgroup managed by the imported projects below.
-->
<Project>
  <Import Project="CopyFile.targets"      Condition="'$(CopyFileTargetsImported)'      == ''" />
  <Import Project="MoveFile.targets"      Condition="'$(MoveFileTargetsImported)'      == ''" />
  <Import Project="ImportFile.targets"    Condition="'$(ImportFileTargetsImported)'    == ''" />
  <Import Project="ImportProject.targets" Condition="'$(ImportProjectTargetsImported)' == ''" />
  <Import Project="Sign.targets"          Condition="'$(SignTargetsImported)'          == ''" />
  <Import Project="Wim.targets"           Condition="'$(WimTargetsImported)'           == ''" />
  <Import Project="Zip.targets"           Condition="'$(ZipTargetsImported)'           == ''" />
</Project>
