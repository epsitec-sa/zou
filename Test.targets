<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Test1" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  
  <!-- Simulates default msbuild variables -->
  <PropertyGroup Condition="'$(SolutionDir)' == ''">
    <SolutionDir>$([System.IO.Directory]::GetParent($(MSBuildProjectDirectory)).FullName)\sal\</SolutionDir>
    <SolutionName>Salaires</SolutionName>
    <SolutionFileName>$(SolutionName).sln</SolutionFileName>
    <SolutionPath>$(SolutionDir)$(SolutionFileName)</SolutionPath>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(ProjectDir)' == ''">
    <ProjectDir>$([System.IO.Directory]::GetParent($(MSBuildProjectDirectory)).FullName)\libefx\</ProjectDir>
    <ProjectName>Efx</ProjectName>
    <ProjectFileName>$(ProjectName).vcxproj</ProjectFileName>
    <ProjectPath>$(ProjectDir)$(ProjectFileName)</ProjectPath>
    
    <Platform>Win32</Platform>
    <Configuration>Debug</Configuration>
    <PlatformToolSet>vc140</PlatformToolSet>
  </PropertyGroup>

  
  
  <!-- Global targets -->
  <ImportGroup Condition="'$(Zou)' == ''">
    <Import Project="private\zou.targets"/>
  </ImportGroup>

  <ImportGroup>
    <Import Project="Cpp.NTVersion.props"/>
    <Import Project="Cpp.OutDir.props"/>
  </ImportGroup>
  
  <Target Name="Test1" DependsOnTargets="ZouTraceGlobals;ZouTraceFallback">
  </Target>
 
  <!-- Test2 -->
  <Target Name="Test2">
    <ZouResolveFallback File="Cpp.NTVersion.props" ZouProjectDir="$(ZouProjectDir)" ZouSolutionDir="$(ZouSolutionDir)" ZouBundleDir="$(ZouBundleDir)" ZouDir="$(ZouDir)">
      <Output PropertyName="ZouVersionFullPath" TaskParameter="FullPath" />
    </ZouResolveFallback>
    <Message Text="ZouVersionFullPath=$(ZouVersionFullPath)" />
  </Target>
</Project>