<?xml version="1.0" encoding="utf-8"?>
<!-- Import this script at the beginning of a C# .pack agent -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ZouAgentFunc>Pack</ZouAgentFunc>
    <_PropertySheetDisplayName>[zou] C# Pack Agent</_PropertySheetDisplayName>
  </PropertyGroup>

  <ImportGroup Label="ZouExtensions">
    <Import Project="Cs.Utility.props" />
  </ImportGroup>

  <ImportGroup Label="PropertySheets">
    <Import Project="Com.WorkDir.props" />
    <Import Project="Cs.IntDir.props" />
    <Import Project="Cs.PkgDir.props" />
    <Import Project="Cs.OutDir.props" />
  </ImportGroup>

  <Import Project="Cs.Utility.targets" />
</Project>