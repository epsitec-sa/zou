<!--
The Cpp.Utility.targets is used to create a minimal C++ utility project (.vcxproj)
with standard configurations (Debug, Release) and platforms (Win32, x64).
For example:

	<?xml version="1.0" encoding="utf-8"?>
	<Project DefaultTargets="Build" ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	  <PropertyGroup Label="Globals">
		  <ProjectGuid>{B78D246D-F9C7-4635-81EF-6B5C25D1D8E8}</ProjectGuid>
		  <PlatformToolset>v120_xp</PlatformToolset>
	  </PropertyGroup>
	  <Import Project="zou\Cpp.Utility.props" />
	  ...
	  <Import Project="zou\Cpp.Utility.targets" />
    ....
	</Project>
-->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets" />

  <!-- Reset TargetDir for C++ utility projects -->
  <!--<PropertyGroup>
    <TargetDir></TargetDir>
  </PropertyGroup>-->
</Project>