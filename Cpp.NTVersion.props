<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet resolves the 'NTVersion'property and C++ '_WIN32_WINNT' macro (supports override in zou.cfg folder). -->
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003"
         InitialTargets="CppNTVersionTrace"
         TreatAsLocalProperty="NTVersion">

  <ImportGroup Label="PropertySheets">
    <Import Project="$(ZouBundleDir)**\$(MSBuildThisFileName)*$(MSBuildThisFileExtension)"   Condition="'$(ZouBundleDir)'   != ''" />
    <Import Project="$(ZouSolutionDir)**\$(MSBuildThisFileName)*$(MSBuildThisFileExtension)" Condition="'$(ZouSolutionDir)' != '' And '$(ZouSolutionDir)' != '$(ZouBundleDir)'" />
    <Import Project="$(ZouProjectDir)**\$(MSBuildThisFileName)*$(MSBuildThisFileExtension)"  Condition="'$(ZouProjectDir)'  != '' And '$(ZouProjectDir)'  != '$(ZouSolutionDir)' And '$(ZouProjectDir)' != '$(ZouBundleDir)'" />
    <Import Project="$(ZouPrivateDir)$(MSBuildThisFileName).Default$(MSBuildThisFileExtension)" />
  </ImportGroup>

  <Choose>
    <When Condition="'$(NTVersion)' == '0x601'">
      <PropertyGroup>
        <_PropertySheetDisplayName>Windows 7 (0x601)</_PropertySheetDisplayName>
      </PropertyGroup>
    </When>
    <When Condition="'$(NTVersion)' == '0x600'">
      <PropertyGroup>
        <_PropertySheetDisplayName>Windows Vista (0x600)</_PropertySheetDisplayName>
      </PropertyGroup>
    </When>
    <When Condition="'$(NTVersion)' == '0x502'">
      <PropertyGroup>
        <_PropertySheetDisplayName>Windows Server 2003 (0x502)</_PropertySheetDisplayName>
      </PropertyGroup>
    </When>
    <When Condition="'$(NTVersion)' == '0x501'">
      <PropertyGroup>
        <_PropertySheetDisplayName>Windows XP (0x501)</_PropertySheetDisplayName>
      </PropertyGroup>
    </When>
    <When Condition="'$(NTVersion)' == '0x500'">
      <PropertyGroup>
        <_PropertySheetDisplayName>Windows 2000 (0x500)</_PropertySheetDisplayName>
      </PropertyGroup>
    </When>
    <Otherwise>
      <PropertyGroup>
        <_PropertySheetDisplayName>ERROR: unknown windows version</_PropertySheetDisplayName>
      </PropertyGroup>
    </Otherwise>
  </Choose>

  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>_WIN32_WINNT=$(NTVersion);%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>

  <Target Name="CppNTVersionTrace" Condition="'$(ZouTraceProps)' == 'true'">
    <Message Importance="normal" Text="NTVersion = _WIN32_WINNT = $(NTVersion)" />
  </Target>
</Project>