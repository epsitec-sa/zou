<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="W1" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\zou.props"/>

  <Target Name="Lock1">
    <Lock Name="$(MSBuildThisFileDirectory)" />
  </Target>
  <Target Name="Unlock1">
    <Unlock Name="$(MSBuildThisFileDirectory)" />
  </Target>
  <Target Name="Sleep1">
    <Message Text="[W1] working..." />
    <Sleep Timeout="500" />
    <!--<Error Text="[W1] forced error!" />-->
    <Sleep Timeout="500" />
    <Message Text="[W1] work done." />
  </Target>
  
  <Target Name="W1">
    <Message Text="[W1] starting..." />
    <CallTarget Targets="Lock1;Sleep1;Unlock1" RunEachTargetSeparately="true"/>
    <Message Text="[W1] exiting..." />
  </Target>
</Project>
