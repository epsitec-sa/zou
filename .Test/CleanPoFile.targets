<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Module" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\zou.props"/>

  <Target Name="Bundle">
    <ItemGroup>
      <PoFile Include="$(BundleDir).po\sync-dev.fr-CH.po">
        <Bundle>$(BundleName)</Bundle>
      </PoFile>
    </ItemGroup>
    <CleanPoFile SourceFiles="@(PoFile)" />
  </Target>

  <Target Name="Module">
    <ItemGroup>
      <PoFile Include="$(ZouDir).po\zou.fr-CH.po">
        <Bundle>$(BundleName)</Bundle>
        <Module>$(ModuleName)</Module>
      </PoFile>
    </ItemGroup>
    <CleanPoFile SourceFiles="@(PoFile)" />
  </Target>

  <Target Name="Domain">
    <ItemGroup>
      <PoFile Include="$(ZouDir).Test\PoResource\Lib1\Resource\Lib1.fr-CH.po">
        <Bundle>$(BundleName)</Bundle>
        <Module>$(ModuleName)</Module>
        <Domain>Lib1</Domain>
      </PoFile>
    </ItemGroup>
    <CleanPoFile SourceFiles="@(PoFile)" />
  </Target>
</Project>