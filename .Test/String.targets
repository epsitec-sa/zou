<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="T1" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Target Name="T1">
    <Message Importance="high" Text="Compare(A, B) = $([System.StringComparer]::OrdinalIgnoreCase.Compare('A', 'B'))" />
    <Message Importance="high" Text="Compare(A, a) = $([System.StringComparer]::OrdinalIgnoreCase.Compare('A', 'a'))" />
    <Message Importance="high" Text="Compare(A, -) = $([System.StringComparer]::OrdinalIgnoreCase.Compare('A', $(DontExist)))" />
  </Target>
</Project>