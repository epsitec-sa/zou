<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet defines the default debug settings for C++ projects. -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  
  <PropertyGroup>
    <EnableDiagnostics>false</EnableDiagnostics>
  </PropertyGroup>

  <PropertyGroup>
    <_PropertySheetDisplayName Condition="'$(EnableDiagnostics)' == 'true'">[sal] Diagnostics ON</_PropertySheetDisplayName>
    <_PropertySheetDisplayName Condition="'$(EnableDiagnostics)' == 'false'">[sal] Diagnostics OFF</_PropertySheetDisplayName>
    <_Diagnostics>true</_Diagnostics>
  </PropertyGroup>

</Project>