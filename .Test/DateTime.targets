<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" DefaultTargets="T1" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <DateTimeMinValue>$([System.DateTime]::MinValue.ToString())</DateTimeMinValue>
  </PropertyGroup>
  <Target Name="T1">
    <Message Importance="high" Text="DateTime.MinValue=$(DateTimeMinValue)" />
  </Target>
</Project>