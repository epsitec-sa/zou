<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" DefaultTargets="W2" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\zou.props"/>

  <Target Name="Lock2">
    <Lock Name="$(MSBuildThisFileDirectory)" />
  </Target>
  <Target Name="Unlock2">
    <Unlock Name="$(MSBuildThisFileDirectory)" />
  </Target>
  <Target Name="Sleep2">
    <Message Text="[W2] working..." />
    <Sleep Timeout="500" />
    <!--<Error Text="[W2] forced error!" />-->
    <Sleep Timeout="500" />
    <Message Text="[W2] work done." />
  </Target>
  
  <Target Name="W2">
    <Message Text="[W2] starting..." />
    <CallTarget Targets="Lock2;Sleep2;Unlock2" RunEachTargetSeparately="true"/>
    <Message Text="[W2] exiting..." />
  </Target>
</Project>
