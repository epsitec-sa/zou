<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\zou.props"/>
  
  <ItemGroup>
    <ImportFile Include="$(ZouDir)**\*.props" Exclude="$(ZouDir)tmp\**\*">
      <ImportDir>$(ZouDir)tmp\.Test\ImportFile\</ImportDir>
    </ImportFile>
  </ItemGroup>
  <Import Project="$(ZouDir)ImportFile.targets" />

  <Target Name="Clean" DependsOnTargets="ImportFileClean" />
  <Target Name="Build" DependsOnTargets="ImportFile" />
</Project>