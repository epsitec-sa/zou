<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" DefaultTargets="UpdateVldPackages" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\..\zou.props"/>

  <PropertyGroup>
    <!-- mock properties normally defined by a C++ project -->
    <SolutionDir       Condition="'$(SolutionDir)'       == ''">.\</SolutionDir>
    <Configuration     Condition="'$(Configuration)'     == ''">Debug</Configuration>
    <IntDir            Condition="'$(IntDir)'            == ''">obj\$(Configuration)\</IntDir>
    <OutDir            Condition="'$(OutDir)'            == ''">bin\$(Configuration)\</OutDir>
    <PlatformToolset   Condition="'$(PlatformToolset)'   == ''">v120_xp</PlatformToolset>
    <ConfigurationType Condition="'$(ConfigurationType)' == ''">StaticLibrary</ConfigurationType>
    <VldVersion        Condition="'$(VldVersion)'        == ''">2.5.0.0</VldVersion>
  </PropertyGroup>

  <Import Project="$(ZouDir)Cpp.Vld.targets"/>

  <Target Name="PrepareForBuild" BeforeTargets="Build" />
  <Target Name="Build" />
</Project>