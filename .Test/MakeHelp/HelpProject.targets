<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="_Init;Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\..\zou.props" />

  <PropertyGroup>
    <IntDir Condition="'$(IntDir)' == ''">zou.tmp\obj\</IntDir>
    <OutDir Condition="'$(OutDir)' == ''">$(MSBuildThisFileDirectory)zou.tmp\bin\</OutDir>
  </PropertyGroup>
  
  <ItemGroup>
    <HelpProject Include="$(BundleDir)libcompta\Help\fr\cresus.fr.hhp">
      <CppResIdsFile>$(BundleDir)libcompta\resource.h</CppResIdsFile>
    </HelpProject>
  </ItemGroup>

  <Import Project="$(ZouDir)Cpp.HelpProject.targets"/>

  <Target Name="_Init">
    <MakeDir Directories="$(IntDir);$(OutDir)" />
  </Target>
  <Target Name="ClCompile" AfterTargets="Build" />
  <Target Name="Build" />
  <Target Name="Clean" />
</Project>