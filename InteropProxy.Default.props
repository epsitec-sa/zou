<?xml version="1.0" encoding="utf-8"?>
<!--
The InteropProxy.Default.props defines the default C++ interop proxy output directory relative to its project folder.
-->
<Project ToolsVersion="4.0" InitialTargets="InteropProxyDefaultTrace" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Condition="'$(Zou)' == ''" Project="zou.props" />

  <PropertyGroup>
    <CppInteropPlatform Condition="'$(CppInteropPlatform)' == ''">Win32</CppInteropPlatform>
    <CppInteropProxyDir Condition="'$(CppInteropProxyDir)' == ''">$(CppInteropPlatform)\iproxy\bin\$(Configuration)\</CppInteropProxyDir>
  </PropertyGroup>

  <!-- ================ InteropTrace ================ -->
  
  <Target Name="InteropProxyDefaultTrace">
    <Message Text="CppInteropPlatform = $(CppInteropPlatform)" />
    <Message Text="CppInteropProxyDir = $(CppInteropProxyDir)" />
  </Target>
</Project>