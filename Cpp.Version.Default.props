<?xml version="1.0" encoding="utf-8"?>
<!--
Import this MSBuild property sheet into your project to define the default 'OutDir' and 'IntDir' properties.
For example:

  <ImportGroup Label="PropertySheets">
    <Import Project="zou\Cpp.OutDir.props" />
  </ImportGroup>


NT4       0x0400
WIN2K     0x0500
WINXP     0x0501
WS03      0x0502
WIN6      0x0600
VISTA     0x0600
WS08      0x0600
LONGHORN  0x0600
WIN7      0x0601

-->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>zou - NT Version (XP)</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>_WIN32_WINNT=0x501;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>