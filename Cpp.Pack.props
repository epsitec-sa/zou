<?xml version="1.0" encoding="utf-8"?>
<!-- Import this script at the beginning of a C++ .pack agent -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>[zou] C++ Pack Agent</_PropertySheetDisplayName>
  </PropertyGroup>
  
  <ImportGroup Label="ZouExtensions">
    <Import Project="Cpp.Agent.props" />
  </ImportGroup>

  <ImportGroup Label="PropertySheets">
    <Import Project="Cpp.NTVersion.props" />
    <Import Project="Com.WorkDir.props"/>
    <Import Project="Tmp.IntDir.props" />
    <Import Project="Cpp.DbgDir.props" />
    <Import Project="Cpp.PkgDir.props" />
    <Import Project="Cpp.OutDir.props"/>
  </ImportGroup>
  <Import Project="Cpp.Agent.targets" />

  <Target Name="PackFilter" />
</Project>