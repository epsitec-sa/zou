<?xml version="1.0" encoding="utf-8"?>
<!-- This property sheet resolves the 'OutDir'property for C++ interop proxy projects (supports override in zou.cfg folder). -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003"
         InitialTargets="CppInteropOutDirTrace">

  <PropertyGroup>
    <_PropertySheetDisplayName>C++ Output Directory: $(ProjectDirectory)$(Platform)\Interop\$(Configuration)\</_PropertySheetDisplayName>
    <OutDir>$(ProjectDirectory)$(Platform)\Interop\$(Configuration)\</OutDir>
  </PropertyGroup>

  <Target Name="CppInteropOutDirTrace">
    <Message Importance="normal" Text="OutDir = $(OutDir)" />
  </Target>
</Project>